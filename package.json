{
  "name": "@blowfishxyz/blocklist",
  "version": "0.0.1",
  "description": "Fetch and execute lookups on Blowfish blocklists",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "lint:prettier": "prettier --write \"{src,test}/**/*.{ts,tsx}\"",
    "lint:prettier:check": "prettier --check \"{src,test}/**/*.{ts,tsx}\"",
    "lint:eslint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint": "npm run lint:eslint && npm run lint:prettier:check",
    "test": "jest"
  },
  "keywords": [],
  "author": "Blowfish team <contact@blowfish.xyz>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/sha1": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "sha1": "^1.1.1"
  }
}
